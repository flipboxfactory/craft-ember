{
  "name": "flipboxfactory/craft-ember",
  "description": "A Craft CMS plugin scaffolding",
  "version": "1.0.2.1",
  "keywords": [
    "flipbox",
    "ember",
    "craft cms"
  ],
  "license": "MIT",
  "authors": [{
    "name": "Flipbox Factory",
    "email": "hello@flipboxfactory.com"
  }],
  "require": {
    "craftcms/cms": "~3.0.0",
    "yiisoft/yii2": "~2.0.15"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "squizlabs/php_codesniffer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "flipbox\\ember\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "flipbox\\ember\\tests\\": "tests/src/"
    }
  }
}
