{
  "name": "flipboxfactory/craft-ember",
  "description": "A Craft CMS plugin scaffolding",
  "version": "3.0.0",
  "keywords": [
    "flipbox",
    "ember",
    "craft cms"
  ],
  "license": "MIT",
  "authors": [{
    "name": "Flipbox Factory",
    "email": "hello@flipboxfactory.com"
  }],
  "require": {
    "php": ">=8.0",
    "craftcms/cms": "^3.0.14|~4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "flipbox\\craft\\ember\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "flipbox\\craft\\ember\\tests\\": "tests/src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "2.3.x-dev",
      "dev-master": "2.2.x-dev"
    }
  }
}
